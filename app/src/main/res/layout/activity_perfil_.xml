<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Perfil_Activity">

    <TextView
            android:text="TextView"
            android:layout_width="198dp"
            android:layout_height="37dp"
            android:id="@+id/textView4" android:layout_marginTop="54dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="107dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="106dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="198dp"
            android:layout_height="37dp"
            android:id="@+id/textView3" android:layout_marginTop="17dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="107dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="106dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/com_facebook_profile_picture_blank_square"
            android:id="@+id/imageView3" android:layout_marginEnd="106dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="106dp" android:layout_marginTop="88dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="TextView"
            android:layout_width="198dp"
            android:layout_height="37dp"
            android:id="@+id/textView" android:layout_marginTop="21dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="106dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="107dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>